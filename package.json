{
  "name": "babel-plugin-material-ui",
  "version": "0.2.3",
  "description": "Babel plugin to cherry-pick used material-ui modules",
  "main": "lib/index.js",
  "repository": "https://github.com/umidbekkarimov/babel-plugin-material-ui.git",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "cover": "nyc npm t -- --reporter=min",
    "report-coverage": "npm run cover && nyc report --reporter=lcov",
    "codecov": "npm run report-coverage > coverage.lcov && codecov",
    "lint": "eslint .",
    "build": "cross-env BABEL_ENV=release babel src --out-dir lib"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.19.0",
    "babel-preset-latest": "6.16.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "cross-env": "3.1.3",
    "eslint": "3.11.1",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-plugin-import": "2.2.0",
    "glob": "7.1.1",
    "material-ui": "0.16.4",
    "mocha": "3.2.0",
    "nyc": "10.0.0",
    "react": "15.4.1"
  }
}
